@model List<practice5.Models.table1>
@{
    ViewBag.Title = "new1ko";
    var sn = 0;
}

<h2>new1ko</h2>

<table border="1">
    <tr>
        <th>SN</th>
        <th>Name</th>
        <th>Address</th>
        <th>Contact</th>
        <th>Edit</th>
    </tr>
    @foreach(var data in Model)
            {
                <tr>
                    <td>@(++sn)</td>
                    <td>@(data.name)</td>
                    <td>@(data.address)</td>
                    <td>@(data.contact)</td>
                    <td><a href="@Url.Action("edit", new { id = data.id})">Edit</td>
                </tr>
            }

</table>
<br /><br />   
<button class="btn1" ><a href="@Url.Action("hewlo")">Add Record</a></button>
<button><a href="@Url.Action("edit")">Edit Data</button>

